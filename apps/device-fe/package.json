{
	"name": "device-fe",
	"private": true,
	"type": "module",
	"dependencies": {
		"@tailwindcss/vite": "^4.0.6",
		"@tanstack/react-query": "^5.90.5",
		"@tanstack/react-devtools": "^0.7.0",
		"@tanstack/react-router": "^1.132.0",
		"@tanstack/react-router-devtools": "^1.132.0",
		"@tanstack/router-plugin": "^1.132.0",
		"lucide-react": "^0.545.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwindcss": "^4.0.6"
	},
	"devDependencies": {
		"@testing-library/dom": "^10.4.0",
		"@testing-library/react": "^16.2.0",
		"@types/node": "^22.10.2",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"@vitejs/plugin-react": "^5.0.4",
		"jsdom": "^27.0.0",
		"typescript": "^5.7.2",
		"vite": "^7.1.7",
		"vitest": "^3.0.5",
		"web-vitals": "^5.1.0"
	},
	"nx": {
		"name": "device-fe",
		"targets": {
			"format": {
				"executor": "nx:run-commands",
				"options": {
					"cwd": "apps/device-fe",
					"command": "biome format",
					"args": [
						"--write"
					]
				}
			},
			"lint": {
				"executor": "nx:run-commands",
				"options": {
					"cwd": "apps/device-fe",
					"command": "biome lint",
					"args": [
						"--write"
					]
				}
			},
			"check": {
				"executor": "nx:run-commands",
				"options": {
					"cwd": "apps/device-fe",
					"command": "biome check",
					"args": [
						"--write"
					]
				}
			},
			"test": {
				"executor": "nx:run-commands",
				"options": {
					"cwd": "apps/device-fe",
					"command": "vitest run"
				}
			}
		}
	}
}
